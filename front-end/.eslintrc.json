{
  "extends": ["next/core-web-vitals", "plugin:import/errors", "plugin:import/warnings"],
  "plugins": ["import"],
  "rules": {
    "import/no-unresolved": "error",
    "no-undef": "error"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [["@", "./"]],
        "extensions": [".js", ".jsx"]
      }
    }
  }
}
